<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.enn.energy.price.dal.mapper.ext.proxyprice.CityCustomMapper">
    <sql id="Base_Column_List">
        id, area_code, name, level, parent_id, state,create_time,update_time
    </sql>

    <select id="queryCityList" resultType="com.enn.energy.price.dal.po.ext.CityCode">
        select
        <include refid="Base_Column_List" />
        from city_code
        where state=#{state}
        <if test="level !=null and level!=''">
            and level=#{level}
        </if>
        <if test="parentId !=null and parentId !=''">
            parent_id=#{parentId}
        </if>
    </select>
</mapper>